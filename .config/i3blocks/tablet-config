# Configurazione di base di i3blocks
interval=1
separator_block_width=10

[cpu_usage]
command=~/.local/statusbar/sb-cpubars
interval=2 

[wifi]
command=~/.local/statusbar/sb-wifi
interval=2

[battery]
command=~/.local/statusbar/sb-battery
interval=2


[ora]
command=~/.local/statusbar/sb-clock
interval=1

[keyboard]
command=~/.local/statusbar/sb-keyboard
interval=10000

[close]
command=~/.local/statusbar/sb-close
interval=10000
